# Stock URLs configuration
# Base URLs for different data sections
YAHOO_FINANCE_QUOTE_URL="https://finance.yahoo.com/quote/{symbol}"
YAHOO_FINANCE_STATS_URL="https://finance.yahoo.com/quote/{symbol}/key-statistics"
YAHOO_FINANCE_FINANCIALS_URL="https://finance.yahoo.com/quote/{symbol}/financials"
YAHOO_FINANCE_ANALYSIS_URL="https://finance.yahoo.com/quote/{symbol}/analysis"
YAHOO_FINANCE_HISTORY_URL="https://finance.yahoo.com/quote/{symbol}/history"

# LLM Configuration
MODEL_NAME="qwen3:14b"
MODEL_TEMPERATURE=0.7
MODEL_MAX_TOKENS=32768  # 32K tokens maximum context window for Qwen3 14B

# Data collection configuration
SCRAPING_TIMEOUT=30
RETRY_ATTEMPTS=2
CONCURRENT_REQUESTS=4
